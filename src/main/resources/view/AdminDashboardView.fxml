<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="504.0" prefWidth="798.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="adminWelcomeMsg" layoutX="50.0" layoutY="22.0" textFill="#083b94">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TableView fx:id="eventsTable" layoutX="14.0" layoutY="100.0" prefHeight="380.0" prefWidth="770.0">
         <columns>
            <TableColumn fx:id="eventCol" prefWidth="309.0" text="Event" />
            <TableColumn fx:id="venueDayPriceCol" minWidth="0.0" prefWidth="248.0" text="Venue, Day with Price" />
            <TableColumn fx:id="soldTicketsCol" prefWidth="103.0" text="Sold Tickets" />
            <TableColumn fx:id="totalTicketsCol" prefWidth="107.0" text=" Total Tickets" />
         </columns>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TableView>
      <Button layoutX="722.0" layoutY="74.0" mnemonicParsing="false" onAction="#adminLogout" prefHeight="20.0" prefWidth="60.0" text="Logout">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="manageEvents" layoutX="111.0" layoutY="74.0" mnemonicParsing="false" onAction="#manageEventsClicked" prefHeight="20.0" prefWidth="81.0" text="Manage Events">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button layoutX="204.0" layoutY="74.0" mnemonicParsing="false" onAction="#viewAllOrders" prefHeight="20.0" prefWidth="81.0" text="All Orders">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button layoutX="20.0" layoutY="74.0" mnemonicParsing="false" onAction="#displayAddEventPopup" prefHeight="20.0" prefWidth="81.0" text="Add Event">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
