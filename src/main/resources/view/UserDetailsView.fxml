<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="326.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="109.0" layoutY="74.0" text="Preffered Name:" />
      <Label fx:id="preferredName" layoutX="252.0" layoutY="74.0" prefHeight="17.0" prefWidth="179.0" text="l" />
      <Button layoutX="122.0" layoutY="286.0" mnemonicParsing="false" onAction="#updatePassword" text="Update Password">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="240.0" layoutY="286.0" mnemonicParsing="false" onAction="#backToDashboard" prefHeight="21.0" prefWidth="92.0" text="Close">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Label layoutX="109.0" layoutY="122.0" text="Need to update Password?">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="109.0" layoutY="156.0" text="Current password:" />
      <PasswordField fx:id="currentPassword" layoutX="253.0" layoutY="152.0" prefHeight="17.0" prefWidth="119.0" />
      <Label layoutX="109.0" layoutY="190.0" text="New password:" />
      <PasswordField fx:id="newPassword" layoutX="253.0" layoutY="186.0" prefHeight="17.0" prefWidth="119.0" />
      <Label layoutX="109.0" layoutY="226.0" text="Confirm New password:" />
      <PasswordField fx:id="confirmNewPassword" layoutX="253.0" layoutY="222.0" prefHeight="17.0" prefWidth="119.0" />
   </children>
</AnchorPane>
